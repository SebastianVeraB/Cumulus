<aura:component>
    
    <!-- Attributes Section -->
    <aura:attribute name="SObjectMapIterable" type="List" default="[]" description="List of SObjects, [{key: name of SObject, value: array of SObjects}]"/>
    <!-- /Attributes Section -->
    
    <lightning:navigation aura:id="navService"/>
    
    <!-- Aura body Section -->
    <div class="c-container slds-p-around_medium">
        <lightning:layout multipleRows="true">
            
            <aura:iteration items="{!v.SObjectMapIterable}" var="SObjectVar" indexVar="index">
                
                <lightning:layoutItem size="12" padding="around-small">
                    <a href="#" class="slds-text-link_faux">
                        <div class="slds-box slds-theme_default" onclick="{!c.navigateTo }" id="{!index}">
                            {!SObjectVar.key} ({!SObjectVar.value.length})
                        </div>
                    </a>
                </lightning:layoutItem>
                
            </aura:iteration>
        </lightning:layout>
    </div>
    <!-- /Aura body Section -->
    
</aura:component>